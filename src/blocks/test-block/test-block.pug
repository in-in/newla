include ../picture/picture.pug

mixin test-block()
  .test-block&attributes(attributes)
    p I am test-block
    .icons
      p icons:
      each i, index in Array(4)
        - var x = index + 1
        div(class='box' + x)= x
    .sharp
      p responsive images:
      +picture([
        {
          "name": "image1",
          "width": [ 900, 700, 500, 300 ]
        }
      ])
